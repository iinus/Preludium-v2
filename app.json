{
  "expo": {
    "name": "Preludium",
    "slug": "Preludium",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/Logo-P.png",
    "splash": {
      "image": "src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "src/assets/Logo-P.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "ts", "tsx", "svg", "image"]
    }
  }
}
